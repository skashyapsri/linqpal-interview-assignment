{
  "name": "Linqpal",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.13",
    "@phuocng/react-pdf-viewer": "^1.7.0",
    "aws-amplify": "^3.0.21",
    "bootstrap": "4.5.0",
    "chart.js": "2.9.3",
    "history": "4.10.1",
    "jwt-decode": "^3.1.2",
    "node-sass": "4.14.1",
    "pdfjs-dist": "^2.4.456",
    "public-ip": "^4.0.2",
    "react": "16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-detect-offline": "^2.4.0",
    "react-device-detect": "^1.13.1",
    "react-dom": "16.13.1",
    "react-google-maps": "9.4.5",
    "react-notification-alert": "0.0.12",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "reactstrap": "8.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "deploy": "aws s3 sync --acl public-read --delete build s3://linqpal-admin && npm run invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EUACOIXAYL8VG --paths '/*'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css",
    "minify-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "@types/googlemaps": "3.39.6",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.35",
    "ajv": "6.12.2",
    "jquery": "3.5.1",
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "axios": "^0.19.2"
  }
}
